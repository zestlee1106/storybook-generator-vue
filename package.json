{
  "name": "test-extension-vue",
  "displayName": "test-extension-vue",
  "description": "",
  "publisher": "ywlee",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "createStory",
        "title": "스토리북 생성"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "createStory",
          "when": "resourceLangId == vue",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "test-extension-vue",
      "properties": {
        "test-extension-vue.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable this extension"
        },
        "test-extension-vue.alias": {
          "type": "object",
          "default": {},
          "description": "alias 설정"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vue/compiler-sfc": "^3.3.13",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-template-compiler": "^2.7.15"
  },
  "dependencies": {
    "vue": "^3.2.36"
  }
}
